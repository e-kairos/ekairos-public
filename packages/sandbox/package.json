{
  "name": "@ekairos/sandbox",
  "version": "1.21.100-beta.0",
  "description": "Pulzar Sandbox - Provider-agnostic sandbox helpers",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-kairos/ekairos-public.git",
    "directory": "packages/sandbox"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js",
      "require": "./dist/app.js",
      "default": "./dist/app.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.js",
      "default": "./dist/runtime.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.js",
      "default": "./dist/schema.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive:true, force:true})\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@daytonaio/sdk": "^0.134.0",
    "@ekairos/domain": "^1.21.100-beta.0",
    "@instantdb/admin": "0.22.126",
    "@instantdb/core": "0.22.126",
    "@vercel/sandbox": "^0.0.23",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@ekairos/tsconfig": "workspace:*",
    "@types/node": "^24.5.0",
    "typescript": "^5.9.2"
  }
}
