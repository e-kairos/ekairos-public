{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "registry:build": {
      "dependsOn": ["^build"],
      "outputs": ["public/r/**"]
    }
  },
  "globalPassThroughEnv": [
    "NODE_ENV"
  ]
}
